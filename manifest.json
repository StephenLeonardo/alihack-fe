{
    "manifest_version": 3,
    "name": "Content Scraper",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["contentScript.js"]
        }
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
  }
  